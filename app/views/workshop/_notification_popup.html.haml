%h4= "Aggiungi segnalazione"
= form_tag( create_workshop_notification_path(@worksheet.number), remote: true, method: :post) do |f|
  = text_field_tag :description, nil, placeholder: 'Descrizione..', class: 'form-control', id: 'focus_element', style: 'width: 80vw;'
  = hidden_field_tag :protocol, protocol
  = submit_tag :ok
